{
    "keyboard.dispatch": "keyCode",
    "breadcrumbs.enabled": false,
    "telemetry.telemetryLevel": "off",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.emptyWindow": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.startupPrompt": "never",
    "window.restoreWindows": "none",
    "window.titleBarStyle": "native",
    "window.customTitleBarVisibility": "never",
    "window.menuBarVisibility": "hidden",
    "window.commandCenter": true,
    "window.density.editorTabHeight": "default",
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.autoDetectColorScheme": true,
    "editor.formatOnSave": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.inlineSuggest.enabled": false,
    "editor.inlineSuggest.edits.showCollapsed": false,
    "editor.inlineSuggest.showToolbar": "never",
    "editor.fontFamily": "'Cascadia Code', 'monospace'",
    "editor.fontSize": 13,
    "editor.lineHeight": 1.3,
    "editor.lineNumbers": "on",
    "editor.renderLineHighlight": "gutter",
    "editor.fontLigatures": true,
    "editor.guides.indentation": false,
    "editor.rulers": [120],
    "editor.folding": false,
    "editor.showFoldingControls": "never",
    "editor.wordWrap": "off",
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 2,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.stickyScroll.enabled": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 50,
    "editor.minimap.autohide": "none",
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.side": "right",
    "editor.hover.enabled": false,
    "editor.lightbulb.enabled": "off",
    "extensions.ignoreRecommendations": true,
    "debug.terminal.clearBeforeReusing": true,
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "x-terminal-emulator",
    "terminal.integrated.fontFamily": "'Cascadia Code', 'monospace'",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.allowChords": false,
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "icon": "terminal-bash",
            "path": ["/bin/bash","/usr/bin/bash"],
            "args": ["--noprofile", "--norc", "--posix"]
        },
        "dash": {
            "icon": "terminal-debian",
            "path": ["/bin/dash","/usr/bin/dash"]
        }
    },
    "workbench.editor.showTabs": "multiple",
    "workbench.startupEditor": "none",
    "workbench.activityBar.location": "bottom",
    "workbench.sideBar.location": "left",
    "workbench.editor.enablePreview": false,
    "workbench.editor.closeEmptyGroups": true,
    "workbench.list.openMode": "doubleClick",
    "workbench.tree.renderIndentGuides": "none",
    "workbench.tree.indent": 20,
    "workbench.preferredDarkColorTheme": "Visual Studio Dark",
    "workbench.preferredLightColorTheme": "Visual Studio Light",
    "workbench.iconTheme": "vs-seti",
    "workbench.productIconTheme": "Default",
    "workbench.layoutControl.enabled": false,
    "chat.commandCenter.enabled": false,
    "github.copilot.editor.enableCodeActions": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "*": false
    },
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticMode": "openFilesOnly",
    "python.analysis.diagnosticsSource": "Pylance",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    }
}
